def zip_lists_user_input():
    m = int(input())
    n = int(input())
    list1 = [[int(input()) for _ in range(n)] for _ in range(m)]
    list2 = [[int(input()) for _ in range(n)] for _ in range(m)]

    zipped_list = [a + b for a, b in zip(list1, list2)]
    return zipped_list
zipped_result = zip_lists_user_input()
print(zipped_result)
