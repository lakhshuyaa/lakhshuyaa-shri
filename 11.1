try:
  num1 = int(input())
  num2 = int(input())

  if num2 == 0:
    print("Error: Cannot divide or modulo by zero.")
  else:
    division_result = num1 / num2
    modulo_result = num1 % num2
    print("Division result:", division_result)
    print("Modulo result:", modulo_result)

except ValueError:
  print("Error: Non-numeric input provided.")
