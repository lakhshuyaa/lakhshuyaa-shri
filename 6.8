def find_pair_with_difference(arr, k):
    seen = set()
    for i in range(len(arr)):
        if (arr[i] - k) in seen or (arr[i] + k) in seen:
            return 1
        seen.add(arr[i])
    return 0
t = int(input())
for _ in range(t):
    n = int(input())
    arr = [int(input()) for _ in range(n)]
    k = int(input())
    result = find_pair_with_difference(arr, k)
    print(result)
